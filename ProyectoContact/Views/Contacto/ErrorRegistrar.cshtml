@model ProyectoContact.Models.Contacto

@{
    ViewBag.Title = "ErrorRegistrar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row" style="margin-top:38px">

    <div class="col-sm-12">
        <div class="panel-body panel-warning">
            <div class="panel-heading">
                <h4>
                    Error
                </h4>
            </div>
            <div class="panel-body">
                <div class="alert alert-danger">
                    <p>Hay campos que se deben completar</p>  
                </div>
                <form action="@Url.Action("ErrorRegistrar","Contacto")">
                    <a href="@Url.Action("Registrar","Contacto")" class="btn btn-warning"> <i class="fa-solid fa-arrow-rotate-left"></i>Volver</a>
                </form>
            </div>
        </div>
    </div>
</div>

